<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>tickets</servlet-name>
    <servlet-class>com.revature.controller.TicketServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>tickets</servlet-name>
    <url-pattern>/tickets/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>user</servlet-name>
    <servlet-class>com.revature.controller.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>user</servlet-name>
    <!-- we include the * meaning anything can come after this -->
    <url-pattern>/user</url-pattern>
  </servlet-mapping>




</web-app>
