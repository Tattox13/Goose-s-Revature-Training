<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>pets</servlet-name>
    <servlet-class>com.revature.controller.PetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pets</servlet-name>
    <url-pattern>/pets/*</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>person</servlet-name>
    <servlet-class>com.revature.controller.PersonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>person</servlet-name>
    <!-- we include the * meaning anything can come after this -->
    <url-pattern>/person</url-pattern>
  </servlet-mapping>


</web-app>
